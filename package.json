{
  "name": "webpack-auto-clean-build-plugin",
  "version": "1.0.4-alpha.1",
  "description": "Automatically removes previous chunk output files if the filename is not same as the new output file name.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "github.com:aikar/webpack-auto-clean-build-plugin"
  },
  "keywords": [
    "webpack",
    "plugin",
    "watch",
    "clean",
    "build"
  ],
  "author": "Daniel Ennis <aikar@aikar.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aikar/webpack-auto-clean-build-plugin/issues"
  },
  "homepage": "https://github.com/aikar/webpack-auto-clean-build-plugin",
  "dependencies": {
    "fs-extra": "^2.0.0"
  },
  "scripts": {
    "install-modules": "npm install",
    "check-modules": "npm-check",
    "update-modules": "npm-check -u --save-exact --skip-unused",
    "clean-modules": "rimraf node_modules/",
    "install-typedefs": "typings install",
    "clean-typedefs": "rimraf typings/",
    "setup": "npm run install-modules && npm run install-typedefs",
    "lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "prebuild": "npm run lint",
    "build": "babel src/ --out-dir dist/ --source-maps && babel test/ --out-dir dist_test/ --source-maps",
    "pretest": "npm run build",
    "test": "mocha dist_test/",
    "clean": "rimraf dist/ dist_test/ test_output"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "chai": "3.5.0",
    "dirty-chai": "1.2.2",
    "eslint-config-standard": "7.0.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "2.1.1",
    "mocha": "3.2.0",
    "npm-check": "5.4.0",
    "rimraf": "2.6.1",
    "standard": "9.0.1",
    "typings": "2.1.0",
    "webpack": "2.2.1"
  }
}
